{
  "version": 2,
  "builds": [
    {
      "src": "Yunwanlol/server.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": "Yunwanlol/templates/**/*"  
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "Yunwanlol/static/$1",
      "status": 404  
    },
    {
      "src": "/(.*)",
      "dest": "Yunwanlol/server.py"  
    }
  ]
}
