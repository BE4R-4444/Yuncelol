{
  "version": 2,
  "builds": [
    {
      "src": "Yunwanlol/server.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": "Yunwanlol/templates/**/*"  // 包含模板文件（如果有）
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Yunwanlol/server.py"  // 所有请求由Flask处理
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/Yunwanlol/server.py"  // 根路径重定向
    }
  ]
}
