{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "includeFiles": "templates/**/*,*.json,static/**/*"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "CDN-Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "server.py",
      "headers": {
        "Cache-Control": "no-cache",
        "CDN-Cache-Control": "no-cache",
        "Vary": "Accept-Encoding"
      }
    }
  ],
  "regions": ["hkg1"],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "framework": {
    "cleanUrls": true,
    "trailingSlash": false
  }
}
